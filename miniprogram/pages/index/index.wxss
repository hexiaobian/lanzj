page{
  width: 90%;
  margin: 0 auto;
}
.title{
  display: flex;
  width: 100%;
  margin-top: 20rpx;
  height: 90rpx;
}
.title view{
  margin-top: 10rpx;
  margin-right: 10rpx;
  padding: 0 10rpx;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #33ccff;
  border: 1px solid #ddd;
  color: white;
}
.map{
  position: relative;
  top: 10vh;
  width: 100%;
}
.map image{
  position: absolute;
  width: 100%;
  border: none;
}
.understand{
