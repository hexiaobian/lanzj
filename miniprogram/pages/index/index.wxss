page {
  width: 90vw;
  height: 100vh;
  margin: 0 auto;
}

.title {
  display: flex;
  width: 100%;
  margin-top: 20rpx;
  height: 90rpx;
}

.title view {
  margin-top: 10rpx;
  margin-right: 10rpx;
  padding: 0 10rpx;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #3cf;
  border: 1px solid #ddd;
  color: white;
}

.map {
  position: relative;
  top: 10vh;
  width: 100%;
}

.map image {
  position: absolute;
  width: 100%;
}

.understand {
  position: relative;
  top: 8vh;
  height: 50vh;
  width: 100%;
}

.understand video {
  width: 100%;
  height: 100%;
}

