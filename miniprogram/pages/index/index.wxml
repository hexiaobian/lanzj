<view class="container">
  <view class="title">
    <view hover-class="click1" bindtap="click" style="background-color:{{vColor1}}" data-id="1">湛江全览</view>
    <view hover-class="click2" bindtap="click" style="background-color:{{vColor2}}" data-id="2">了解湛江</view>
  </view>
  <view hidden="{{pre1}}" class="map">
    <image src="../../images/zhanjiang.png" mode="widthFix"></image>
